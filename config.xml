<?xml version='1.0' encoding='utf-8'?>
<widget id="com.net.banacoo.app.yayannet" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>蕉庫</name>
    <description>
      蕉庫无人便利店
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/drawable-ldpi/icon.png" />
        <icon density="mdpi" src="res/icon/android/drawable-mdpi/icon.png" />
        <icon density="hdpi" src="res/icon/android/drawable-hdpi/icon.png" />
        <icon density="xhdpi" src="res/icon/android/drawable-xhdpi/icon.png" />
        <icon density="xxhdpi" src="res/icon/android/drawable-xxhdpi/icon.png" />
        <icon density="xxxhdpi" src="res/icon/android/drawable-xxxhdpi/icon.png" />
        <splash density="port-ldpi" src="res/screen/android/drawable-ldpi/port-screen.png" />
        <splash density="port-hdpi" src="res/screen/android/drawable-hdpi/port-screen.png" />
        <splash density="port-mdpi" src="res/screen/android/drawable-mdpi/port-screen.png" />
        <splash density="port-xhdpi" src="res/screen/android/drawable-xhdpi/port-screen.png" />
        <uses-permission name="android.permission.READ_PHONE_STATE" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-app-version" spec="^0.1.9" />
    <plugin name="cordova-plugin-qrscanner" spec="^2.5.0" />
    <plugin name="cordova-plugin-x-toast" spec="^2.6.2" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="8000" />
    <plugin name="cordova-plugin-yy-bestpay" spec="/Users/yayan/Documents/cordova/plugins/BestPay" />
    <engine name="android" spec="^6.2.3" />
</widget>
